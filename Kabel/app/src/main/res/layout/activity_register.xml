<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.asuspc.kabel.Register">

    <EditText
        android:id="@+id/etRemail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="97dp"
        android:ems="10"
        android:hint="Email"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/etRpass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/etRemail"
        android:layout_alignStart="@+id/etRemail"
        android:layout_below="@+id/etRemail"
        android:layout_marginTop="41dp"
        android:ems="10"
        android:hint="Katasandi"
        android:inputType="textPassword" />

    <Button
        android:id="@+id/btRegister"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/etRpass"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="53dp"
        android:text="Lanjut" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btRegister"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="46dp"
        android:text="Sudah daftar ? Klik disini"
        tools:text="Sudah daftar ? klik disini" />
</RelativeLayout>
